{
  "name": "Desktop Selector",
  "description": "Use to desktop selector for sharing the screens",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
  },
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [ {
    "js": [ "content.js" ],
    "matches": [ "https://*vidya.io/*" ]
  }]
}
